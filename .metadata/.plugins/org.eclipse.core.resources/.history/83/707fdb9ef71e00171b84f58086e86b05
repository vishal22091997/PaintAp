package testing;

import java.awt.Color;
import java.awt.Point;

public class Eraser {
	private Color color;
	private Point corner;
	private int rad;
	public Color getColor() {
		return color;
	}
	public void setColor(Color color) {
		this.color = color;
	}
	public Point getCorner() {
		return corner;
	}
	public void setCorner(Point corner) {
		this.corner = corner;
	}
	public int getRad() {
		return rad;
	}
	public void setRad(int rad) {
		this.rad = rad;
	}
	public Eraser(Color color, Point corner, int rad) {
		super();
		this.color = color;
		this.corner = corner;
		this.rad = rad;
	}

}
